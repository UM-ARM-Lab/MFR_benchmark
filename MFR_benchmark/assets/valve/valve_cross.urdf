<?xml version="1.0" ?>

<robot name="valve_on_wall">
<link name="wall">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0.0"/>
      <mass value="10000.0"/>
      <inertia ixx="1000.0" ixy="0.0" ixz="0.0" iyy="1000.0" iyz="0.0" izz="1000.0"/>
    </inertial>
</link>

<joint name="wall_valve_mount" type="revolute">
    <parent link="wall"/>
    <child link="valve"/>
    <!-- <origin rpy="0 0 0" xyz="0 -0.151 0.7"/>
    <axis xyz="0 1 0"/> -->
    <origin rpy="-1.5707 0 0" xyz="0 -0.151 0.0"/>
    <axis xyz="0 0 1"/>
    <!-- <limit effort="300" lower="-2.9670597283903604" upper="2.9670597283903604" velocity="1.7453292519943295"/> -->
    <dynamics damping="0.5"/>
</joint>

<link name="valve">
    <inertial>
      <!-- <origin rpy="0 0 0" xyz="0 0 0"/> -->
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.03"/>
      <!-- <friction value="1.0"/> -->
      <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
    </inertial>
    <visual>
        <geometry>
            <cylinder radius="0.01" length="0.2"/>
        </geometry>
           <material name="Red1">
	 	<color rgba="0.9 0. 0. 0.5"/>
	</material>
    </visual>
    <collision>
        <geometry>
            <cylinder radius="0.01" length="0.2"/>
        </geometry>
    </collision>
</link>

<joint name="valve_cross_mount_1" type="fixed">
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 -0.1"/>
    <parent link="valve"/>
    <child link="cross_1"/>
</joint>

<link name="cross_1">
    <inertial>
      <mass value="0.03"/>
      <!-- <inertia ixx="0.00001" ixy="0.0" ixz="0.0" iyy="0.000001" iyz="0.0" izz="0.000001"/> -->
    </inertial>
    <visual>
        <origin rpy="0 0 0" xyz="0 0 0.0"/>
        <geometry>
            <box size="0.2 0.02 0.02"/>
        </geometry>
        <material name="Blue">
	 	    <color rgba="0. 0.2 0.8 0.5"/>
	    </material>
    </visual>
    <collision>
        <origin rpy="0 0 0" xyz="0 0 0.0"/>
        <geometry>
            <box size="0.2 0.02 0.02"/>
        </geometry>
    </collision>
</link>

<joint name="valve_cross_mount_2" type="fixed">
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 -0.1"/>
    <parent link="valve"/>
    <child link="cross_2"/>
</joint>

<link name="cross_2">
    <inertial>
      <mass value="0.03"/>
      <!-- <inertia ixx="0.00001" ixy="0.0" ixz="0.0" iyy="0.000001" iyz="0.0" izz="0.000001"/> -->
    </inertial>
    <visual>
        <origin rpy="0 0 0" xyz="0 0 0.0"/>
        <geometry>
            <box size="0.02 0.2 0.02"/>
        </geometry>
        <material name="Blue">
	 	    <color rgba="0. 0.2 0.8 0.5"/>
	    </material>
    </visual>
    <collision>
        <origin rpy="0 0 0" xyz="0 0 0.0"/>
        <geometry>
            <box size="0.02 0.2 0.02"/>
        </geometry>
    </collision>

</link>





</robot>